using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Globalization;

class Calculator
{
    static void Main(string[] args)
    {
        int operand = int.Parse(Console.ReadLine());
        string operatorSTR = Console.ReadLine();
        int secondOperand = int.Parse(Console.ReadLine());
        int result = new int();
        
        if (operatorSTR == "+")
        {
            result = operand + secondOperand;
        }
        else if (operatorSTR == "-")
        {
            result = operand + secondOperand;
        }
        else if (operatorSTR == "*")
        {
            result = operand * secondOperand;
        }
        else if (operatorSTR == "/")
        {
    
            {
                result = operand / secondOperand;
            }
        }
        Console.WriteLine($"{operand} {operatorSTR} {secondOperand} = {result}");
    }
}
